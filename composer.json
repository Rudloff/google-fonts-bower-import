{
  "require": {
    "gitonomy/gitlib": "~1.0.2",
    "knplabs/github-api": "~2.0.0@beta",
    "php-http/guzzle6-adapter": "~1.1.1",
    "google/fonts": "dev-master",
    "symfony/yaml": "~3.2.0"
  },
  "require-dev": {
    "symfony/var-dumper": "~3.2.0",
    "squizlabs/php_codesniffer": "~2.7.1",
    "phpunit/phpunit": "~5.7.2"
  },
  "name": "google-fonts-bower/import",
  "description": "Tools used to convert Google Fonts to Bower packages",
  "license": "GPL-3.0",
  "autoload": {
    "psr-4": {
      "GoogleFontsBower\\": "classes/"
    }
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "google/fonts",
        "version": "dev-master",
        "dist": {
          "url": "https://github.com/google/fonts/archive/master.zip",
          "type": "zip"
        }
      }
    }
  ],
  "scripts": {
    "import": "php import.php",
    "register": "php register.php"
  }
}
